 (num) => this.gameVar.get(`Player_No${num}`).getChild(\"Name\").getAsString();","  setPlayerName = (num,str) => this.gameVar.get(`Player_No${num}`).getChild(\"Name\").setString(str);","","  getPlayerColor = (num) => this.gameVar.get(`Player_No${num}`).getChild(\"Color\").getAsNumber();","  setPlayerColor = (num,value) => this.gameVar.get(`Player_No${num}`).getChild(\"Color\").setNumber(value);","","  update(){","    [this.player1Status,this.player2Status,this.player3Status] = [this.getPlayerStatus(1),this.getPlayerStatus(2),this.getPlayerStatus(3)];","    [this.player1Color,this.player2Color,this.player3Color] = [this.getPlayerColor(1),this.getPlayerColor(2),this.getPlayerColor(3)];","    [this.player1Name,this.player2Name,this.player3Name] = [this.getPlayerName(1),this.getPlayerName(2),this.getPlayerName(3)];","  }","","  applyData(lobby){","    for(var i = 1; i <= 3; i++){","        this.setPlayerStatus(i,lobby[`Player${i}Status`]);","        this.setPlayerColor(i,lobby[`Player${i}Color`]);","        this.setPlayerName(i,lobby[`Player${i}Name`]);","    }","  }","","  toJSON() {","    return JSON.stringify({","      TYPE: 'LOBBY_STATUS',","      LOBBY: {","          Player1Status: this.player1Status,","          Player2Status: this.player2Status,","          Player3Status: this.player3Status,","          Player1Name: this.player1Name,","          Player2Name: this.player2Name,","          Player3Name: this.player3Name,","          Player1Color: this.player1Color,","          Player2Color: this.player2Color,","          Player3Color: this.player3Color,","      }","    });","  }","}","","class Client{","    constructor(hostIp){","        this.lobby = new LobbyStatus();","        const client = dgram.createSocket('udp4');","        this.port = 54122;","        this.hostIp;","        this.connectorList = [];","        client.bind(this.port)","        this.client = client;","        // for dev testing","        // this.hostLobby()","    }","    setHostIp(ip){","        this.hostIp = ip;","    }","    sendToAll(data){","        for(var i in this.connectorList){","            var ip = this.connectorList[i];","            this.client.send(data,this.port,ip)","        }","    }","    sendToAllExcept(data,exceptIp){","        for(var i in this.connectorList){","            if(i === exceptIp) return;","            var ip = this.connectorList[i];","            this.client.send(data,this.port,ip)","        }","    }","    sendStart(){","        const gameStatus = runtimeScene.getGame().getVariables().get('Game_Status').getAsString();","        const data = JSON.stringify({","            TYPE: 'GAME_STATUS',","            GameStatus: gameStatus,","        })","        // const data = ","        this.sendToAll(data);","    }","    addConnector(newIp){","        // prevent split digit bug","        if(newIp.length < 8) return;","        // check list includes newIp","        if( this.connectorList.indexOf(newIp) !== -1) return;","        this.connectorList.push(newIp);","    }","    ","    hostLobby(){","        console.log('lobby host')","        this.client.on('message',(msg,rinfo)=>{","            const data = JSON.parse(msg.toString())","            if(data.TYPE == 'REQUEST')","                {","                    const lobbystat = new LobbyStatus().toJSON();","                    const res = JSON.stringify({","                        TYPE: 'RESPONSE',","                        CONNECTOR_LIST : `${this.connectorList}`,","                        Lobby: lobbystat,","                    })","                    this.addConnector(rinfo.address)","                    this.client.send(res,this.port,rinfo.address)","                    console.log(`PLAYER JOINED: ${rinfo.address}`)","                }","            if(data.TYPE === 'REQUEST_LOBBY'){","               this.sendLobbyStatus(rinfo.address)","            }","            if(data.TYPE === 'LOBBY_STATUS'){","                const lobby = data.LOBBY;","                this.lobby.appl